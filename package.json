{
  "name": "krig-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for static projects",
  "main": "index.js",
  "repository": "https://gitlab.com/lucaskrig/krig-boilerplate",
  "author": "Lucas Meijer <lucas@krig.io>",
  "license": "MIT",
  "private": true,
  "meta-tags": {
    "viewport": "width=device-width, initial-scale=1, shrink-to-fit=no",
    "keywords": "",
    "og:url": "",
    "og:image": "",
    "og:description": "",
    "og:title": "",
    "og:site_name": "",
    "og:see_also": "",
    "name": "",
    "description": "",
    "image": "",
    "favicon": "favicon.png",
    "twitter:card": "",
    "twitter:url": "",
    "twitter:description": "",
    "twitter:image": "",
    "robots": "",
    "charset": "utf-8"
  },
  "config": {
    "title": "Static Boilerplate"
  },
  "scripts": {
    "dev": "webpack-serve ./src/build/webpack.config-dev.js --hot --log-time",
    "start": "yarn build && yarn dev",
    "prebuild": "rm -rf dist",
    "build":
      "yarn prebuild && webpack --progress --config src/build/webpack.config-dev.js",
    "build:prod": "webpack --progress --config src/build/webpack.config-prod.js"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "bs-html-injector": "^3.0.3",
    "cache-loader": "^1.2.2",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^1.1.11",
    "fs": "^0.0.1-security",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.1.5",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^1.0.2",
    "yarn": "^1.6.0"
  }
}
